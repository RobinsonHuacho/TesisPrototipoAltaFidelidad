<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.usuario.pruebas.RegistroBeneficiario"
    android:background="@color/mpsdk_white">


    <TextView
        android:id="@+id/textView14"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="24dp"
        android:layout_marginRight="24dp"
        android:text="POR FAVOR INGRESE SUS DATOS PERSONALES"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="24dp"
        app:layout_constraintHorizontal_bias="0.0"
        android:textColor="@color/mpsdk_dark_font_color"
        android:layout_marginStart="24dp"
        android:layout_marginEnd="24dp" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="24dp"
        android:text="CÉDULA"
        android:textSize="18sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView14"
        android:textColor="@color/mpsdk_dark_font_color"
        android:layout_marginStart="24dp" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="PRIMER NOMBRE"
        android:textSize="18sp"
        android:layout_marginLeft="24dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@+id/textView15"
        android:textColor="@color/mpsdk_dark_font_color"
        android:layout_marginStart="24dp" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="SEGUNDO NOMBRE"
        android:textSize="18sp"
        android:layout_marginLeft="24dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@+id/textView16"
        android:textColor="@color/mpsdk_dark_font_color"
        android:layout_marginStart="24dp" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="PRIMER APELLIDO"
        android:textSize="18sp"
        android:layout_marginLeft="24dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@+id/textView17"
        android:textColor="@color/mpsdk_dark_font_color"
        android:layout_marginStart="24dp" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="SEGUNDO APELLIDO"
        android:textSize="18sp"
        android:layout_marginLeft="24dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@+id/textView18"
        android:textColor="@color/mpsdk_dark_font_color"
        android:layout_marginStart="24dp" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="DIRECCIÓN"
        android:textSize="18sp"
        android:layout_marginLeft="24dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:textColor="@color/mpsdk_dark_font_color"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@+id/textView19"
        android:layout_marginStart="24dp" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TELÉFONO"
        android:textSize="18sp"
        android:layout_marginLeft="24dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="23dp"
        app:layout_constraintTop_toBottomOf="@+id/textView21"
        android:textColor="@color/mpsdk_dark_font_color"
        android:layout_marginStart="24dp" />

    <EditText
        android:id="@+id/editText_CedulaBeneficiario"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginRight="24dp"
        android:layout_marginStart="24dp"
        android:ems="10"
        android:hint="Ingrese su cédula"
        android:inputType="number"
        android:padding="10dp"
        android:textColor="@color/mpsdk_dark_font_color"
        android:textSize="18sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView15"
        app:layout_constraintLeft_toRightOf="@+id/textView15"
        app:layout_constraintRight_toRightOf="parent" />

    <EditText
        android:id="@+id/editText_PrimerNombreBeneficiario"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Ingrese su primer nombre"
        android:inputType="textPersonName"
        android:textSize="18sp"
        android:padding="10dp"
        android:layout_marginRight="24dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/textView16"
        android:layout_marginLeft="24dp"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView16"
        android:textColor="@color/mpsdk_dark_font_color"
        android:layout_marginStart="24dp"
        android:layout_marginEnd="24dp" />

    <EditText
        android:id="@+id/editText_SegundoNombreBeneficiario"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Ingrese su segundo nombre"
        android:inputType="textPersonName"
        android:textSize="18sp"
        android:padding="10dp"
        android:layout_marginRight="24dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/textView17"
        android:layout_marginLeft="24dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView17"
        android:textColor="@color/mpsdk_dark_font_color"
        android:layout_marginStart="24dp"
        android:layout_marginEnd="24dp" />

    <EditText
        android:id="@+id/editText_PrimerApellidoBeneficiario"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Ingrese su primer apellido"
        android:inputType="textPersonName"
        android:textSize="18sp"
        android:padding="10dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView18"
        android:layout_marginRight="24dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/textView18"
        android:layout_marginLeft="24dp"
        app:layout_constraintHorizontal_bias="0.0"
        android:textColor="@color/mpsdk_dark_font_color"
        android:layout_marginStart="24dp"
        android:layout_marginEnd="24dp" />

    <EditText
        android:id="@+id/editText_SegundoApellidoBeneficiario"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="Ingrese su segundo apellido"
        android:inputType="textPersonName"
        android:textSize="18sp"
        android:padding="10dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView19"
        android:layout_marginRight="24dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/textView19"
        android:layout_marginLeft="24dp"
        android:textColor="@color/mpsdk_dark_font_color"
        android:layout_marginStart="24dp"
        android:layout_marginEnd="24dp" />

    <EditText
        android:id="@+id/editText_Direccion"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="24dp"
        android:layout_marginRight="24dp"
        android:ems="10"
        android:padding="10dp"
        android:hint="Ingrese su dirección"
        android:inputType="textPersonName"
        android:textColor="@color/mpsdk_dark_font_color"
        android:textSize="18sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView21"
        app:layout_constraintLeft_toRightOf="@+id/textView21"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginStart="24dp"
        android:layout_marginEnd="24dp" />

    <EditText
        android:id="@+id/editText_Telefono"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginRight="24dp"
        android:layout_marginStart="24dp"
        android:ems="10"
        android:hint="Ingrese su teléfono"
        android:inputType="number"
        android:padding="10dp"
        android:textColor="@color/mpsdk_dark_font_color"
        android:textSize="18sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView22"
        app:layout_constraintLeft_toRightOf="@+id/textView22"
        app:layout_constraintRight_toRightOf="parent" />

    <Button
        android:id="@+id/button2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginRight="24dp"
        android:layout_marginStart="24dp"
        android:onClick="continuarRegistroBeneficiario"
        android:text="CONTINUAR"
        android:textColor="@color/mpsdk_dark_font_color"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:gravity="center"
        android:text="Términos y condiciones"
        android:textColor="#0066CC"
        tools:layout_editor_absoluteY="424dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@+id/editText_Telefono" />

</android.support.constraint.ConstraintLayout>
